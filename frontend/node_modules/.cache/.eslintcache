                      <Route path=\"/signup\" component={Signup} token={access_token} />\n                        <PrivateRoute path=\"/createmovie\" component={CreateMovie} token={access_token} />  \n                        <PrivateRoute path=\"/editmovie\" component={EditMovie} token={access_token} />                          \n                        </Switch>\n                    \n              \n       \n        </>\n    )\n}\nfunction LoginRoute({ path, component: Component, token, ...props }) {\n    return (\n        <Route\n            {...props}\n            path={path}\n            render={(props) =>\n                token ? <Redirect to=\"/\" /> : <Component {...props} />\n            }\n        />\n    );\n}\nfunction PrivateRoute({ path, component: Component, token, ...props }) {\n    return (\n        <Route\n            {...props}\n            path={path}\n            render={(props) =>\n                token ? <Component {...props} /> : <Redirect to=\"/signin\" />\n            }\n        />\n    );\n}","/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/redux/reducer.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/components/Navbar.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/redux/action-types.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/pages/EditMovie.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/pages/Signin.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/pages/CreateMovie.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/pages/Signup.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/pages/Home.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/components/styled/GlobalStyle.style.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/frontend/src/components/styled/Navbar.style.js",[],"/Users/chrishar/Documents/Work/Movie-DB-With-Redux/fron